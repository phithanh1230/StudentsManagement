@{
    ViewBag.Title = "Index";
}
@model IEnumerable<StudentsManagement.DAO.FindStudents_Result>

<div class="container">
    <div class="search-box">
        <h1><i class="fas fa-user-graduate"></i>Students Management</h1>
        @using (Html.BeginForm("Index", "Students"))
        {
            <div class="form-group">
                <input class="form-control input-search" placeholder="Find studens..." type="text" name="name">
                <input class="btn btn-search" type="submit" value="Search">
            </div>
        }
    </div>
</div>
